<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <title>Deflector</title>
  </head>
  <body>
    <% if(htmlWebpackPlugin.options.environment === 'prod'){ %>
      <!-- React -->
      <script
              crossorigin
              src="https://unpkg.com/react@16/umd/react.production.min.js"
      ></script>
      <script
              crossorigin
              src="https://unpkg.com/react-dom@16/umd/react-dom.production.min.js"
      ></script>
      <script src="//cdn.jsdelivr.net/npm/faunadb@latest/dist/faunadb-min.js"></script>
    <% } else { %>
      <!-- React -->
      <script
              crossorigin
              src="https://unpkg.com/react@16/umd/react.development.js"
      ></script>
      <script
              crossorigin
              src="https://unpkg.com/react-dom@16/umd/react-dom.development.js"
      ></script>
      <script src="//cdn.jsdelivr.net/npm/faunadb@latest/dist/faunadb.js"></script>
    <% } %>

    <script
            crossorigin
            src="https://cdnjs.cloudflare.com/ajax/libs/react-router-dom/5.1.2/react-router-dom.min.js"
    ></script>

    <div id="root"></div>
  </body>
</html>
